    <!-- Start Card Style -->
    <div class="rbt-blog-area rbt-section-overlayping-top rbt-section-gapBottom">
        <div class="container">

            <!-- Start Card Area -->
            <div class="row g-5">
                <!-- Start Single Card  -->
                <div class="col-lg-6 col-md-12 col-sm-12 col-12" data-sal-delay="150" data-sal="slide-up" data-sal-duration="800">
                    <div class="rbt-card variation-02 height-330 rbt-hover">
                        <div class="rbt-card-img">
                            <a href="/blogDetails?id={{post._id}}">
                                <img src="admin/assets/ProductImages/{{post.image}}" alt="Youtube cashcow Blog">
                        </div>
                        <div class="rbt-card-body">
                            <h3 class="rbt-card-title"><a href="/blogDetails?id={{post._id}}">{{post.heading}}</a></h3>
                            <p class="rbt-card-text">{{post.author}}</p>
                            <div class="rbt-card-bottom">
                                <a class="transparent-button" href="/blogDetails?id={{post._id}}">Learn More<i><svg width="17" height="12" xmlns="http://www.w3.org/2000/svg"><g stroke="#27374D" fill="none" fill-rule="evenodd"><path d="M10.614 0l5.629 5.629-5.63 5.629"/><path stroke-linecap="square" d="M.663 5.572h14.594"/></g></svg></i></a>
                            </div>
                        </div>
                    </div>
                </div>
                <!-- End Single Card  -->

                <div class="col-lg-6 col-md-12 col-sm-12 col-12" data-sal-delay="150" data-sal="slide-up" data-sal-duration="800">
                    {{#each posts}}

                    <!-- Start Single Card  -->
                    <div class="rbt-card card-list variation-02 rbt-hover mb-5">
                        <div class="rbt-card-img">
                            <a  href="/blogDetails/?id={{_id}}">
                                <img src="admin/assets/ProductImages/{{image}}" alt="Card image"> </a>
                        </div>
                        <div class="rbt-card-body">
                            <h5 class="rbt-card-title"><a  href="/blogDetails/?id={{_id}}">{{heading}}</a></h5>
                            <div class="rbt-card-bottom">
                                <a class="transparent-button"  href="/blogDetails/?id={{_id}}">Read Article<i><svg width="17" height="12" xmlns="http://www.w3.org/2000/svg"><g stroke="#27374D" fill="none" fill-rule="evenodd"><path d="M10.614 0l5.629 5.629-5.63 5.629"/><path stroke-linecap="square" d="M.663 5.572h14.594"/></g></svg></i></a>
                            </div>
                        </div>
                    </div>
                    <!-- End Single Card  -->
                    {{/each}}
                   
                </div>
            </div>
            <!-- End Card Area -->

            <!-- Start Card Area -->
            <div class="row g-5 mt--15">

            {{#each postlist}}

                <!-- Start Single Card  -->
                <div class="col-lg-4 col-md-6 col-sm-12 col-12">
                    <div class="rbt-card variation-02 rbt-hover">
                        <div class="rbt-card-img">
                            <a  href="/blogDetails/?id={{_id}}">
                                <imgsrc="admin/assets/ProductImages/{{image}}" alt="Card image"> </a>
                        </div>
                        <div class="rbt-card-body">
                            <h5 class="rbt-card-title"><a href="/blogDetails/?id={{_id}}">{{heading}}</a></h5>
                            <p class="rbt-card-text"></p>
                            <div class="rbt-card-bottom">
                                <a class="transparent-button"  href="/blogDetails/?id={{_id}}">Read Article<i><svg width="17" height="12" xmlns="http://www.w3.org/2000/svg"><g stroke="#27374D" fill="none" fill-rule="evenodd"><path d="M10.614 0l5.629 5.629-5.63 5.629"/><path stroke-linecap="square" d="M.663 5.572h14.594"/></g></svg></i></a>
                            </div>
                        </div>
                    </div>
                </div>
                <!-- End Single Card  -->
                {{/each}}

            </div>
            <!-- End Card Area -->

        

        </div>
    </div>
    <!-- End Card Style -->
