  <script>
    document.addEventListener('DOMContentLoaded', () => {
      const allowCookiesButton = document.getElementById('allow-cookies');
      const popup = document.getElementById('popup');

      allowCookiesButton.addEventListener('click', () => {
        // Save the user's preference in a cookie or local storage
        // This example uses localStorage
        localStorage.setItem('cookiesAccepted', true);
        popup.style.display = 'none';

        // Set the "SameSite" attribute for the cookie
        const cookieOptions = {
          sameSite: 'Strict', // or 'Lax' depending on your requirements
          secure: true, // Only send the cookie over HTTPS
        };
        console.log(JSON.stringify(cookieOptions));
        // Send a fetch request to the backend
        fetch('/allow-cookies', {
          method: 'POST',
          credentials: 'same-origin', // Send cookies only on the same site
          headers: {
            'Content-Type': 'application/json',
          },
          body: JSON.stringify(cookieOptions),
        })
          .then(response => {
            // Handle the response if needed
          })
          .catch(error => {
            console.error('An error occurred:', error);
          });
      });

      // Check if the user has already accepted cookies
      const cookiesAccepted = localStorage.getItem('cookiesAccepted');
      if (!cookiesAccepted) {
        // If cookies have not been accepted, show the popup
        popup.style.display = 'flex';
      }
    });


    function showLoadingScreen() {
      const loadingScreen = document.getElementById('loadingscreen');
      loadingScreen.style.display = 'block';
    }

    function hideLoadingScreen() {
      const loadingScreen = document.getElementById('loadingscreen');
      const screeenloadingScreen = document.querySelector('#shooping');
      const displaybanner = document.querySelector('#shoopingbanner');
      const footer = document.querySelector('.footer');
      footer.style.display = 'block';
      loadingScreen.style.display = 'none';
      screeenloadingScreen.style.display = 'block';
      displaybanner.style.display = 'block';
    }

    function handleWindowLoad() {
      if (window.location.href === 'https://vidventuresyt.com/shop') {
        showLoadingScreen();
        setTimeout(hideLoadingScreen, 300);
      }
    }

    function preventContextMenu(event) {
      event.preventDefault();
    }

    document.addEventListener('contextmenu', preventContextMenu);
    window.addEventListener('load', handleWindowLoad);
  </script>


